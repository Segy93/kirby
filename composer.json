{
    "name": "segy93/kirby",
    "description": "Kese za Kirby",
    "version": "1.1.12",
    "keywords": [],
    "license": "UNLICENSED",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "9.*",
        "doctrine/dbal": "^2.5",
        "intervention/image": "^2.4",
        "piwik/device-detector": "^3.11",
        "vlucas/phpdotenv": "^5.5",
        "laravel-doctrine/orm": "1.8.*",
        "mattketmo/email-checker": "^2.1",
        "minishlink/web-push": "^8.0",
        "picqer/php-barcode-generator": "^2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
