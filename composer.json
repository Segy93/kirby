{
    "name": "segy93/kirby",
    "description": "Kese za kirby",
    "version": "1.1.12",
    "keywords": [],
    "license": "UNLICENSED",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.7.*",
        "vlucas/phpdotenv": "~2.2",
        "laravel-doctrine/orm": "1.4.*",
        "doctrine/dbal": "^2.5",
        "intervention/image": "^2.4",
        "picqer/php-barcode-generator": "^0.2.2",
        "mattketmo/email-checker": "^1.5",
        "minishlink/web-push": "^5.2",
        "piwik/device-detector": "^3.11"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
